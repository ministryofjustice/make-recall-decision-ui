<li>
    <h2 class="moj-task-list__section" id='heading-risk-profile'>
        Risk profile
    </h2>
    <ul class="moj-task-list__items">
        <li class="moj-task-list__item">
            <a class="moj-task-list__task-name" aria-describedby='task-list-status-currentRoshForPartA'
               href="{{ changeLinkUrl({ pageUrlSlug: 'rosh', urlInfo: urlInfo, fromAnchor: 'heading-risk-profile' }) }}">{{ pageHeadings.rosh }}</a>
            <span id='task-list-status-currentRoshForPartA'>{{ taskStatus({ hasData: taskCompleteness.statuses.currentRoshForPartA }) }}</span>
        </li>
        <li class="moj-task-list__item">
            <a class="moj-task-list__task-name" aria-describedby='task-list-status-mappa'
               href="{{ changeLinkUrl({ pageUrlSlug: 'mappa', urlInfo: urlInfo, fromAnchor: 'heading-risk-profile' }) }}">{{ pageHeadings.mappa }}</a>
            <span id='task-list-status-mappa'>{{ taskStatus({ hasBeenReviewed: taskCompleteness.statuses.mappa }) }}</span>
        </li>
    </ul>
</li>
